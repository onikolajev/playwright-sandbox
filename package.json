{
  "name": "playwright-sandbox",
  "version": "1.0.0",
  "description": "Oleg is playing arround with playwright in his own sandbox",
  "main": "index.js",
  "scripts": {
    "test": "npx playwright test",
    "test:debug": "npx playwright test --debug",
    "test:visual": "npx playwright test --config=visual.config.ts --reporter=list",
    "test:visual:update-snapshots": "npx playwright test --config=visual.config.ts --update-snapshots",
    "test:api": "npx playwright test --config=api.config.ts --reporter=list",
    "test:show-report": "npx playwright show-report"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@playwright/test": "^1.29.2"
  }
}
